<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adform.trackingsdk.simpleapp">

    <application
        android:name="com.adform.trackingsdk.demoapp.DemoApp"
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">

        <activity
            android:exported="true"
            android:name="com.adform.trackingsdk.demoapp.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- Todo Make sure you're using your own ID here
        Find more info: https://firebase.google.com/docs/admob/android/quick-start#add-app-id-to-androidmanifest-->
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>

    </application>

</manifest>
